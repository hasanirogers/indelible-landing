<%- include('partials/head.ejs', { title: title, bodyClass: bodyClass }) %>
  <h1><%= title %></h1>
  <h2>&lsaquo; <a href="/dashboard?page=1&limit=10"> Back to Dashboard</a></h2>
  <form id="create" action="/create" method="POST">
    <p><sl-input name="firstName" label="First name."></sl-input></p>
    <p><sl-input name="lastName" label="Last name."></sl-input></p>
    <p><sl-input name="email" type="email" label="Email." placeholder="user@domain.com"></sl-input></p>
    <p><indelible-page-type></indelible-page-type></p>
    <p><sl-textarea name="message" label="Gift message."></sl-textarea></p>
    <br />
    <hr />
    <section class="submit">
      <div><sl-button type="submit" variant="primary">Create Page</sl-button></div>
      <span class="message"></span>
    </section>
  </form>
<%- include('partials/footer.ejs') %>
