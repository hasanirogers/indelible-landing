<%- include('partials/head.ejs', { title: title, bodyClass: bodyClass }) %>
  <indelible-landing
    url="<%= page.mediaURL %>"
    media="<%= page.mediaType %>"
    email="<%= page.email %>"
    first-name="<%= page.firstName %>"
    last-name="<%= page.lastName %>"
    <% if (page.message !== '') { %>message="<%= page.message %>"<% } %>
    <% if (page.websiteLink) { %>website-link="<%= page.websiteLink %>"<% } %>
    <% if (page.websiteLinkLabel) { %>website-link-label="<%= page.websiteLinkLabel %>"<% } %>
  >
  </indelible-landing>
<%- include('partials/footer.ejs', { title: title }) %>
